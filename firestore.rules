rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{documentPath=**} {
    	match /feedback/{feedbackId} {
      	allow create: if request.auth.uid != null;
      }
      match /campaign/{campaignId} {
        allow get, list;
      }
      match /donations/{id} {
        allow get, list;
      }
    }
  }
}